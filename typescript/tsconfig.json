{
    "compilerOptions": {
        "module": "nodenext",
        "moduleResolution": "nodenext",
        "resolveJsonModule": true,
        "allowJs": false,
        "removeComments": true,
        "esModuleInterop": false,
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": true,
        "downlevelIteration": true,
        "strict": false,
        "declaration": true,
        "outDir": "dist"
    }
}
